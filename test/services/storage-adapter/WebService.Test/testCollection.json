            "name": "Get service status",
            "event": [
                {
                    "listen": "test",
                    "script": {
                        "id": "159eea66-217d-4bb9-ab27-e035c679d82b",
                        "exec": [
                            "pm.test(\"Check if response is 200\", function() {",
                            "    pm.response.to.have.status(200);",
                            "});",
                            "",
                            "pm.test(\"Check if status is healthy\", function() {",
                            "    var jsonData = pm.response.json();",
                            "    pm.expect(jsonData.Status.IsHealthy).to.equal(true);",
                            "});"
                        ],
                        "type": "text/javascript"
                    }
                }
            ],
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{base_url}}v1/status",
                    "host": [
                        "{{base_url}}v1"
                    ],
                    "path": [
                        "status"
                    ]
                }
            },
            "response": []
        }
    ],
    "protocolProfileBehavior": {}
