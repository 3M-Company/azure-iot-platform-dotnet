                "[resourceId('Microsoft.KeyVault/vaults', parameters('resourceName'))]"
            ],
            "properties": {
                "attributes": {
                    "enabled": true
                },
                "value": "DEFAULT_VALUE"
            }
        },
        {
            "type": "Microsoft.KeyVault/vaults/secrets",
            "apiVersion": "2016-10-01",
            "name": "[concat(parameters('resourceName'), '/identityGatewayPublicKey')]",
            "location": "centralus",
            "dependsOn": [
                "[resourceId('Microsoft.KeyVault/vaults', parameters('resourceName'))]"
            ],
            "properties": {
