                },
                "value": "DEFAULT_VALUE"
            }
        },
        {
            "type": "Microsoft.KeyVault/vaults/secrets",
            "apiVersion": "2016-10-01",
            "name": "[concat(parameters('resourceName'), '/storageAccountConnectionString')]",
            "location": "centralus",
            "dependsOn": [
                "[resourceId('Microsoft.KeyVault/vaults', parameters('resourceName'))]"
            ],
